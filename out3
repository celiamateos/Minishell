#Pa detectar leaks y movidas: valgrind --leak-check=full ./minishell^M$
CCLANG = -lreadline ^M$
#Esta mierda creo que hace falta pa -lreadline en 42^M$
RM = rm -f^M$
LIBFT_DIR = libft/^M$
LIBFT = $(LIBFT_DIR)libft.a^M$
SRC_DIR = src^M$
OBJ_DIR = obj^M$
INCLUDE = include/minishell.h include/minishell2.h^M$
SRC = $(shell find $(SRC_DIR) -name '*.c')^M$
OBJ = $(patsubst $(SRC_DIR)/%.c,$(OBJ_DIR)/%.o,$(SRC))^M$
CHANGES_MADE = 0^M$
CHARS_LEN := 0^M$
CHARS := 0^M$
progress := 4^M$
^M$
all: $(NAME)^M$
	@if [ "$(CHANGES_MADE)" -eq "0" ]; then \^M$
		echo "$(COLOR_RED)No hay cambios para hacer. $(COLOR_RESET)"; \^M$
	fi^M$
	$(call print_progress)^M$
	$(eval progress := 50)^M$
	$(call print_progress)^M$
	$(eval progress := 100)^M$
	$(call print_progress)^M$
	@echo ""^M$
^M$
$(NAME):$(OBJ) $(LIBFT) $(INCLUDE)^M$
	@$(CC) $(CFLAGS) $(OBJ) $(CCLANG) $(LIBFT) -o $(NAME)^M$
	@$(eval CHANGES_MADE=1)^M$
^M$
$(LIBFT):^M$
	@make -C $(LIBFT_DIR) > /dev/null^M$
^M$
define print_progress^M$
	@printf "\r$(COLOR_GREEN)[$(COLOR_GREEN_N) %d%%%*.*s $(COLOR_GREEN)] $(COLOR_PURPLE_N)Minishell $(COLOR_PURPLE)Compiling ðM-^_M-^[ ï¸M-^O$(COLOR_RESET)" $(progress) $(CHARS_LEN) $(CHARS)^M$
endef^M$
^M$
$(OBJ_DIR)/%.o: $(SRC_DIR)/%.c^M$
	@mkdir -p $(dir $@)^M$
	@$(CC) $(CFLAGS) -c -o $@ $<^M$
	$(eval progress=$(shell echo $$(($(progress) + 1))))^M$
	$(call print_progress)^M$
# @echo "$(COLOR_BLUE) Created! ðM-^_M-^X¸ $(COLOR_RESET)"^M$
^M$
clean:^M$
	@rm -rf $(OBJ_DIR) $(LIBFT) > /dev/null^M$
	@make clean -C $(LIBFT_DIR) > /dev/null^M$
^M$
fclean: clean^M$
	@rm -f $(NAME)^M$
	@make fclean -C $(LIBFT_DIR) > /dev/null^M$
	@echo "$(COLOR_RED_N) Cleaned all! ðM-^_§¹ $(COLOR_RESET)"^M$
^M$
normi:^M$
	norminette^M$
^M$
re: fclean all^M$
^M$
.PHONY: all, clean, fclean, re^M$
^M$
^M$
